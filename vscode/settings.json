{
  "[python]": {
    "editor.formatOnType": true
  },
  "editor.hover.delay": 150,
  "editor.quickSuggestionsDelay": 0,
  "editor.smoothScrolling": true,
  "editor.tabCompletion": "on",
  "files.autoSave": "onFocusChange",
  "terminal.integrated.inheritEnv": false,
  "git.autofetch": true,
  "git.enableSmartCommit": true,
  "python.testing.pytestEnabled": true,
  "git.showInlineOpenFileAction": false,
  "explorer.confirmDragAndDrop": false,
  "files.exclude": {
    "**/__pycache__": true,
    "**/.classpath": true,
    "**/.factorypath": true,
    "**/.project": true,
    "**/.settings": true
  },
  "editor.semanticHighlighting.enabled": false,
  "jupyter.askForKernelRestart": false,
  "jupyter.allowUnauthorizedRemoteConnection": true,
  "debug.console.fontSize": 16,
  "terminal.integrated.fontSize": 14,
  "workbench.colorCustomizations": {
    "tab.activeBorderTop": "#0A84FF",
    "editor.background": "#0a0c0e",
    "terminal.background": "#0a0c0e",
    "terminal.foreground": "#dadada",
    "sideBar.background": "#0a0c0e",
    "activityBar.background": "#0a0c0e",
    "statusBar.background": "#0a0c0e",
    "editorGroupHeader.tabsBackground": "#0a0c0e",
    "editorBracketHighlight.foreground1": "#e6ca2e",
    "editorBracketHighlight.foreground2": "#da70d6",
    "editorBracketHighlight.foreground3": "#87cefa",
    "editorBracketHighlight.foreground4": "#f08080",
    "editorBracketHighlight.foreground5": "#00d18f",
    "editorBracketHighlight.foreground6": "#abb2bf"
  },
  "editor.tokenColorCustomizations": {
    "strings": "#55d635",
    "numbers": "#f19640",
    "variables": "#ff5a7e",
    "keywords": "#D55FDE",
    "[One Dark Pro]": {
      "textMateRules": [
        {
          "scope": "source.python",
          "settings": {
            "foreground": "#e6e6e6"
          }
        },
        {
          "scope": "entity.name.type.class",
          "settings": {
            "foreground": "#61afef"
          }
        },
        {
          "scope": "string.quoted.docstring.multi.python",
          "settings": {
            "foreground": "#89b57e"
          }
        },
        {
          "scope": [
            "variable.other.constant",
            "constant.other.caps.python",
            "constant.language.python"
          ],
          "settings": {
            "foreground": "#f19640"
          }
        }
      ]
    }
  },
  "editor.mouseWheelZoom": true,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.list.horizontalScrolling": true,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook",
    "git-rebase-todo": "default"
  },
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "oneDarkPro.editorTheme": "One Dark Pro",
  "workbench.preferredDarkColorTheme": "One Dark Pro",
  "git.confirmSync": false,
  "github.copilot.enable": {
    "*": true,
    "plaintext": true,
    "markdown": true,
    "scminput": false,
    "yaml": true,
    "python": true,
    "toml": false,
    "rust": true
  },
  "debug.console.acceptSuggestionOnEnter": "on",
  "python.formatting.provider": "black",
  "workbench.tree.indent": 14,
  "security.workspace.trust.untrustedFiles": "open",
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "files.associations": {
    "*.py": "python"
  },
  "editor.fontFamily": "'JetBrains Mono', Menlo, Monaco, 'Courier New', monospace",
  "workbench.colorTheme": "One Dark Pro",
  "oneDarkPro.vivid": true,
  "oneDarkPro.bold": true,
  "autoDocstring.startOnNewLine": true,
  "editor.suggestSelection": "recentlyUsed",
  "notebook.output.textLineLimit": 50,
  "editor.minimap.enabled": false,
  "audioCues.volume": 0,
  "editor.accessibilitySupport": "off",
  "git.ignoreRebaseWarning": true,
  "python.analysis.typeCheckingMode": "basic",
  "vim.normalModeKeyBindings": [
    {
      "before": [
        "C-d"
      ],
      "after": [
        "C-d",
        "z",
        "z"
      ]
    },
    {
      "before": [
        "C-u"
      ],
      "after": [
        "C-u",
        "z",
        "z"
      ]
    }
  ],
  "editor.inlineSuggest.enabled": true,
  "jupyter.interactiveWindow.textEditor.executeSelection": true,
  "jupyter.interactiveWindow.textEditor.autoMoveToNextCell": false,
  "jupyter.interactiveWindow.codeLens.commands": "jupyter.runcell,  jupyter.runallcellsabove, jupyter.debugcell, jupyter.runcellandallbelow",
  "editor.lineNumbers": "relative",
  "[toml]": {
    "editor.defaultFormatter": "tamasfe.even-better-toml"
  },
  "editor.stickyScroll.enabled": true,
  "workbench.enableExperiments": false,
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "remote.SSH.logLevel": "trace",
  "ruff.importStrategy": "useBundled",
  "githubPullRequests.pullBranch": "never",
  "autoDocstring.docstringFormat": "numpy",
  "jupyter.debugJustMyCode": false,
  "jupyter.runStartupCommands": [
    "%load_ext autoreload",
    "%autoreload 2"
  ],
  "editor.multiCursorModifier": "ctrlCmd",
  "vscode-neovim.highlightGroups.highlights": {
    
    
    "IncSearch": {
      "backgroundColor": "theme.editor.findMatchBackground",
      "borderColor": "theme.editor.findMatchBorder"
    },
    "Search": {
      "backgroundColor": "theme.editor.findMatchHighlightBackground",
      "borderColor": "theme.editor.findMatchHighlightBorder"
    },
    "Visual": {
      "backgroundColor": "theme.editor.selectionBackground"
    }
  },
  "diffEditor.ignoreTrimWhitespace": false,
}
