{
  "[python]": {
    "editor.formatOnType": true,
    "editor.defaultFormatter": "charliermarsh.ruff"
  },
  "editor.hover.delay": 150,
  "editor.quickSuggestionsDelay": 0,
  "editor.smoothScrolling": true,
  "editor.tabCompletion": "on",
  "files.autoSave": "onFocusChange",
  "terminal.integrated.inheritEnv": false,
  "git.autofetch": true,
  "git.enableSmartCommit": true,
  "python.testing.pytestEnabled": true,
  "git.showInlineOpenFileAction": false,
  "explorer.confirmDragAndDrop": false,
  "files.exclude": {
    "**/__pycache__": true,
    "**/.classpath": true,
    "**/.factorypath": true,
    "**/.project": true,
    "**/.settings": true
  },
  "editor.semanticHighlighting.enabled": false,
  "jupyter.askForKernelRestart": false,
  "jupyter.allowUnauthorizedRemoteConnection": true,
  "debug.console.fontSize": 16,
  "terminal.integrated.fontSize": 14,
  "workbench.colorCustomizations": {
    "tab.activeBorderTop": "#0A84FF",
    "editor.background": "#050505",
    "editor.foreground": "#dadada",
    "terminal.background": "#050505",
    "terminal.foreground": "#dadada",
    "sideBar.background": "#050505",
    "activityBar.background": "#050505",
    "titleBar.activeBackground": "#050505",
    "editorGroupHeader.tabsBackground": "#050505",
    "statusBar.background": "#050505",
    "tab.inactiveBackground": "#050505",
    "statusBar.noFolderBackground": "#050505",
    "tab.activeBackground": "#1b2025",
    "editorBracketHighlight.foreground1": "#e6ca2e",
    "editorBracketHighlight.foreground2": "#da70d6",
    "editorBracketHighlight.foreground3": "#87cefa",
    "editorBracketHighlight.foreground4": "#f08080",
    "editorBracketHighlight.foreground5": "#00d18f",
    "editorBracketHighlight.foreground6": "#abb2bf",
    "editorLineNumber.activeForeground": "#e6e6e6",
    "editorLineNumber.foreground": "#abb2bf",
    "editor.findMatchBackground": "#fff67150",
    "editor.findMatchHighlightBorder": "#ffee00"
  },
  "editor.tokenColorCustomizations": {
    "strings": "#55d635",
    "numbers": "#f19640",
    "variables": "#ff5a7e",
    "keywords": "#D55FDE",
    "[One Dark Pro]": {
      "textMateRules": [
        {
          "scope": "source.python",
          "settings": {
            "foreground": "#e6e6e6"
          }
        },
        {
          "scope": "entity.name.type.class",
          "settings": {
            "foreground": "#61afef"
          }
        },
        {
          "scope": "string.quoted.docstring.multi.python",
          "settings": {
            "foreground": "#89b57e"
          }
        },
        {
          "scope": [
            "variable.other.constant",
            "constant.other.caps.python",
            "constant.language.python"
          ],
          "settings": {
            "foreground": "#f19640"
          }
        }
      ]
    }
  },
  "editor.mouseWheelZoom": true,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.list.horizontalScrolling": true,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook",
    "git-rebase-todo": "default"
  },
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "oneDarkPro.editorTheme": "One Dark Pro",
  "workbench.preferredDarkColorTheme": "One Dark Pro",
  "git.confirmSync": false,
  "github.copilot.enable": {
    "*": true,
    "plaintext": true,
    "markdown": true,
    "scminput": false,
    "yaml": true,
    "python": true,
    "toml": true,
    "rust": true
  },
  "debug.console.acceptSuggestionOnEnter": "on",
  "workbench.tree.indent": 14,
  "security.workspace.trust.untrustedFiles": "open",
  "terminal.integrated.enableMultiLinePasteWarning": "never",
  "files.associations": {
    "*.py": "python"
  },
  "editor.fontFamily": "'JetBrainsMono NF', Menlo, Monaco, 'Courier New', monospace",
  "workbench.colorTheme": "One Dark Pro",
  "oneDarkPro.vivid": true,
  "oneDarkPro.bold": true,
  "autoDocstring.startOnNewLine": true,
  "editor.suggestSelection": "recentlyUsed",
  "notebook.output.textLineLimit": 50,
  "editor.minimap.enabled": false,
  "editor.accessibilitySupport": "off",
  "git.ignoreRebaseWarning": true,
  "python.analysis.typeCheckingMode": "basic",
  "editor.inlineSuggest.enabled": true,
  "jupyter.interactiveWindow.textEditor.executeSelection": true,
  "jupyter.interactiveWindow.textEditor.autoMoveToNextCell": false,
  "jupyter.interactiveWindow.codeLens.commands": "jupyter.runcell,  jupyter.runallcellsabove, jupyter.debugcell, jupyter.runcellandallbelow",
  "editor.lineNumbers": "relative",
  "[toml]": {
    "editor.defaultFormatter": "tamasfe.even-better-toml"
  },
  "editor.stickyScroll.enabled": true,
  "workbench.enableExperiments": false,
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  // "remote.SSH.logLevel": "trace",
  "ruff.importStrategy": "useBundled",
  "githubPullRequests.pullBranch": "never",
  "autoDocstring.docstringFormat": "numpy",
  "jupyter.runStartupCommands": [
    "%load_ext autoreload",
    "%autoreload 2"
  ],
  "editor.multiCursorModifier": "ctrlCmd",
  "diffEditor.ignoreTrimWhitespace": false,
  "window.density.editorTabHeight": "compact",
  "vscode-neovim.ctrlKeysForNormalMode": [
    "a",
    "c",
    "d",
    "e",
    "f",
    "h",
    "i",
    "j",
    "k",
    "l",
    "o",
    "r",
    "t",
    "u",
    "v",
    "w",
    "x",
    "y",
    "z",
    "/",
    "]",
    "right",
    "left",
    "up",
    "down",
    "backspace",
    "delete"
  ],
  "debug.allowBreakpointsEverywhere": true,
  "workbench.editor.showTabs": "single",
  "python.analysis.autoFormatStrings": true,
  "python.terminal.activateEnvironment": false,
  "luahelper.format.column_limit": 140,
  "luahelper.format.break_after_table_lb": true,
  "luahelper.format.break_before_table_rb": true,
  "luahelper.format.chop_down_table": true,
  "luahelper.format.double_quote_to_single_quote": true,
  "zenMode.hideLineNumbers": false,
  "zenMode.showTabs": "none",
  "accessibility.signalOptions": {
    "volume": 0
  },
  "interactiveWindow.executeWithShiftEnter": true,
  "ruff.nativeServer": true,
  "editor.defaultFormatter": "charliermarsh.ruff",
  "notebook.defaultFormatter": "charliermarsh.ruff",
  "vim.sneak": true,
  "vim.easymotion": true,
  "vim.leader": " ",
  "vim.sneakUseIgnorecaseAndSmartcase": true,
  "vim.targets.enable": true,
  "vim.commandLineModeKeyBindings": [

  ],
  "vim.handleKeys": {
    "<C-d>": true,
    "<C-s>": false,
    "<C-z>": false,
    "<C-p>": false,
    "<C-b>": false,
  },
  "vim.highlightedyank.enable": true,
  "vim.easymotionMarkerForegroundColorOneChar": "#ff00e6",
  "vim.normalModeKeyBindings": [
    {
      "before": [
        "<leader>",
        "o"
      ],
      "after": [
        "o",
        "<Esc>"
      ]
    },
    {
      "before": [
        "<leader>",
        "O"
      ],
      "after": [
        "O",
        "<Esc>"
      ]
    },
    {
      "before": [
        "g",
        "t"
      ],
      "commands": [
        "editor.action.peekTypeDefinition",
      ]
    }
  ],
}
